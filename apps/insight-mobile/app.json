{
  "expo": {
    "name": "Insight",
    "slug": "insight-mobile",
    "owner": "qolity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "insight-mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1020"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dgu.insight-mobile",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Insight uses your microphone to capture voice notes and quick thoughts.",
        "NSCalendarsUsageDescription": "Insight syncs with your calendar to help you track and plan your day.",
        "NSRemindersUsageDescription": "Insight can create reminders to help you stay on track with habits and tasks.",
        "NSHealthShareUsageDescription": "Insight reads health data to track your wellness metrics and fitness progress.",
        "NSHealthUpdateUsageDescription": "Insight writes workout data to help you maintain comprehensive health records.",
        "NSLocationWhenInUseUsageDescription": "Insight uses your location to tag entries with places you visit.",
        "UIBackgroundModes": ["audio", "fetch", "remote-notification"]
      },
      "associatedDomains": ["applinks:insight.qolity.io"],
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0B1020"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dgu.insightmobile",
      "versionCode": 1,
      "permissions": [
        "RECORD_AUDIO",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "insight.qolity.io",
              "pathPrefix": "/app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#6366F1"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": ""
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    }
  }
}
